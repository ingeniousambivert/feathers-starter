{
	"name": "server",
	"description": "Feathers Server for Chartbee Team Tool",
	"version": "1.0.0",
	"homepage": "",
	"private": true,
	"main": "src",
	"keywords": [
		"feathers"
	],
	"author": {
		"name": "Monarch Maisuriya",
		"email": "monarchmaisuriya7600@gmail.com"
	},
	"contributors": [],
	"bugs": {},
	"directories": {
		"lib": "src",
		"test": "test/",
		"config": "config/"
	},
	"engines": {
		"node": "^16.0.0",
		"npm": ">= 3.0.0"
	},
	"scripts": {
		"test": "npm run lint && npm run mocha",
		"lint": "eslint --fix .",
		"format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettier.config.js",
		"dev": "nodemon --trace-deprecation --trace-warnings src/server.js | npx bunyan -o short",
		"start": "node src/server.js | npx bunyan -o short",
		"mocha": "mocha test/ --recursive --exit",
		"migrate:up": "npx sequelize db:migrate",
		"migrate:down": "npx sequelize db:migrate:undo"
	},
	"_moduleAliases": {
		"@app": "src/app",
		"@channels": "src/channels",
		"@database": "src/database",
		"@errors": "src/errors",
		"@hooks": "src/hooks",
		"@helpers": "src/helpers",
		"@integrations": "src/integrations",
		"@models": "src/models",
		"@middleware": "src/middleware",
		"@services": "src/services",
		"@utils": "src/utils"
	},
	"standard": {
		"env": [
			"mocha"
		],
		"ignore": []
	},
	"dependencies": {
		"@feathersjs/authentication": "^4.5.11",
		"@feathersjs/authentication-local": "^4.5.11",
		"@feathersjs/configuration": "^4.5.11",
		"@feathersjs/errors": "^4.5.11",
		"@feathersjs/express": "^4.5.11",
		"@feathersjs/feathers": "^4.5.11",
		"@feathersjs/transport-commons": "^4.5.11",
		"@sendgrid/mail": "^7.6.0",
		"bunyan": "^1.8.15",
		"compression": "^1.7.4",
		"cors": "^2.8.5",
		"dotenv": "^10.0.0",
		"feathers-authentication-hooks": "^1.0.2",
		"feathers-authentication-management": "^3.1.0",
		"feathers-hooks-common": "^5.0.6",
		"feathers-permissions": "^2.1.4",
		"feathers-sequelize": "^6.2.0",
		"helmet": "^4.6.0",
		"lodash": "^4.17.21",
		"module-alias": "^2.2.2",
		"morgan": "^1.10.0",
		"pg": "^8.6.0",
		"rate-limiter-flexible": "^2.3.5",
		"sequelize": "^6.6.5",
		"sequelize-cli": "^6.2.0",
		"superagent": "^6.1.0",
		"uuid": "^8.3.2"
	},
	"devDependencies": {
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-prettier": "^4.0.0",
		"faker": "^5.5.3",
		"mocha": "^8.4.0",
		"nodemon": "^2.0.12"
	}
}
